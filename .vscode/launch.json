{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Server",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/bin/server",
            "args": [
                "-d",
                "${workspaceFolder}/tests/dbs/example"
            ],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "windows": {
                "miDebuggerPath": "C:\\tools\\msys64\\mingw64\\bin\\gdb.exe",
            },
            "linux": {
                "miDebuggerPath": "/usr/bin/gdb",
            },
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
